searchlog = VIEW @"Views/SearchLog.view";

rs1 = 
    SELECT Start, Region, MyHelper.SecondsToMinutes(Duration) AS DurationInMinutes
    FROM searchlog;
 
OUTPUT rs1
TO @"/my/Scope.Tutorial/Outputs/output.txt";


#CS
public static class MyHelper
{
    public static double SecondsToMinutes(int seconds)
    {
        double minutes = seconds/60.0;
        return minutes;
    }
}
#ENDCS
