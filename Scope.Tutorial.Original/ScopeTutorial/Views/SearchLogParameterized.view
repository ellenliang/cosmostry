CREATE VIEW SearchLog SCHEMA ( 
	IId:int, 
	UId:int, 
	TimeStamp:DateTime, 
	Market:string, 
	Query:string, 
	DwellTime:int, 
	Results:string, 
	ClickedUrls:string 
)
PARAMS (
	start string,
	end string
)
AS BEGIN

    searchlog = VIEW "SearchLog.view";

    filtered_searchlog = SELECT * 
                FROM searchlog
                WHERE TimeStamp >= DateTime.Parse(@start) AND TimeStamp < DateTime.Parse(@end).AddDays(1);
END;
